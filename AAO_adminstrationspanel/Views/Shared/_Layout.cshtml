                                    <img src="~/image/icon/arrow.svg" class="img-arrow" />
                                </a>

                               <li class="nav-item">
                                <a class="nav-link active text-light" asp-area="" asp-controller="Home" asp-action="OpretTure">
                                @*<a class="nav-link active" href="Home/OpretTur">*@
                                    <img src="~/image/icon/opret.png" class="img-icon" />
                                    Opret tur
                                </a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link nav-bottom">
                                    <img src="~/image/icon/minimere.png" class="img-icon" />
                                    Minimere menu
                                    <img src="~/image/icon/arrow-left.svg" class="" />
                                </a>
                            </li>


                        </ul>
                    </nav>
                </div>

                <div class="col-10 pl-5">
                    <main role="main" class="main">
                        @RenderBody()
                    </main>
                </div>
            </div>
        </div>


