                                    <img src="~/image/icon/arrow.svg" class="img-arrow" />
                                </a>

                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="TildelteTure">
                                @*<a class="nav-link text-dark" href="Home/TildelteTure">*@
