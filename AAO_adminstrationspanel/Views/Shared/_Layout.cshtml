                        <ul>
                            <li class="nav-item ">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="AlleTure">
                                    @*<a class="nav-link text-dark" href="AlleTure">*@
                                    <img src="~/image/icon/alleture.png" width="25" height="20" style="margin: 8px;" />
